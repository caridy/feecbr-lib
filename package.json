{
  "name": "photonews-lib",
  "version": "0.0.1",
  "description": "photonews micro lib",
  "dependencies": {
    "debug": "*"
  },
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib",
  "peerDependencies": {
    "express": "3.x",
    "express-annotations": "*",
    "express-map": "*",
    "express-state": "*",
    "express-view": "*",
    "express-yui": "~1.0.0",
    "yui": "~3.13.0"
  },
  "devDependencies": {
    "jshint": "~1.1.0",
    "yui-lint": "~0.1.3"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/jshint --config ./node_modules/yui-lint/jshint.json ./lib/*.js *.js",
    "pretest": "npm run lint"
  },
  "locator": {
    "-rulesets": "app-rulesets.js",
    "-location": "."
  },
  "repository": "https://github.com/imalberto/photonews-lib",
  "author": "alberto chan <imalberto@gmail.com>",
  "license": "BSD"
}
